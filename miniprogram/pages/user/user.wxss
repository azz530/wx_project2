.pages{
  display: flex;
  flex-direction: column;
  background-color: rgb(250, 250, 250);
}
.head{
  height: 150rpx;
  width: 60%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  margin-top: 60rpx;
  padding-left: 100rpx;
}
.unlogin{
  margin-top: 15rpx;
}
.unlogin image{
  width: 120rpx;
  height: 120rpx;
  background-color: rgb(134, 199, 255);
  border-radius: 50%;
}
.login{
  font-size: 40rpx;
  line-height: 150rpx;

}
.userM{
  display: flex;
  flex-direction: row;
  margin-left: 150rpx;
  margin-top: 60rpx;
}
.userPic{
  text-align: center;
}
.userPic image{
  border-radius: 50%;
  width: 150rpx;
  height: 150rpx;
}
.userName{
  font-size: 50rpx;
  margin-top: 40rpx;
}
.bar{
  width: 100%;
  height: 700rpx;
  margin-top: 50rpx;
  background-color: rgb(255, 255, 255);

}
.own{
  height: 100rpx;
  margin-top: 30rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  border-radius: 30rpx;
}
.bartext{
  position: relative;
  left: 30rpx;
  top: 20rpx;
}
.own image{
  margin-top: 25rpx;
  width: 45rpx;
  height: 45rpx;
}
.divline{
  background-color: rgb(228, 228, 228);
  height: 1rpx;
  width: 98%;
  margin-left: 5rpx;
}
.logout{
  margin-top: 50rpx;
}
.logout button{
  border: 1rpx solid rgb(187, 223, 240);
  border-radius: 50rpx;
  background-color: rgb(255, 255, 255);
  color: rgb(134, 199, 255);
}